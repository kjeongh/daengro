{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/daengro/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/daengro/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/daengro",
  "projects": {
    "daengro": {
      "type": "application",
      "root": "apps/daengro",
      "entryFile": "main",
      "sourceRoot": "apps/daengro/src",
      "compilerOptions": {
        "tsConfigPath": "apps/daengro/tsconfig.app.json"
      }
    },
    "grant": {
      "type": "application",
      "root": "apps/grant",
      "entryFile": "main",
      "sourceRoot": "apps/grant/src",
      "compilerOptions": {
        "tsConfigPath": "apps/grant/tsconfig.app.json"
      }
    },
    "apply": {
      "type": "application",
      "root": "apps/apply",
      "entryFile": "main",
      "sourceRoot": "apps/apply/src",
      "compilerOptions": {
        "tsConfigPath": "apps/apply/tsconfig.app.json"
      }
    },
    "packages": {
      "type": "library",
      "root": "libs/packages",
      "entryFile": "index",
      "sourceRoot": "libs/packages/src",
      "compilerOptions": {
        "tsConfigPath": "libs/packages/tsconfig.lib.json"
      }
    },
    "kafka": {
      "type": "library",
      "root": "libs/kafka",
      "entryFile": "index",
      "sourceRoot": "libs/kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka/tsconfig.lib.json"
      }
    },
    "storage": {
      "type": "library",
      "root": "libs/storage",
      "entryFile": "index",
      "sourceRoot": "libs/storage/src",
      "compilerOptions": {
        "tsConfigPath": "libs/storage/tsconfig.lib.json"
      }
    }
  }
}